@model IEnumerable<MedCore.Web.Models.Medical.SpecialtiesModels.SpecialtiesModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SpecialtiesId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SpecialtyName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatedAt)
